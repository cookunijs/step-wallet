{
  "expo": {
    "name": "contract-wallet",
    "slug": "contract-wallet",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "config": "./rn-cli.config.js"
    },
    "ios": {
      "bundleIdentifier": "com.daikikunii.MyContractWalletApp",
      "supportsTablet": true
    },
    "android": {
      "package": "com.daikikunii.MyContractWalletApp",
      "permissions": []
    }
  }
}